@model InicializarTablasVista
@{
    ViewBag.Title = "Inicializar Tablas";
    ViewBag.SubMenuSelected = "InicializarTablas";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var acciones = ViewData["Acciones"] as IList<string>;
}
@section Scripts{
    @Content.Script("SGE.InicializarTablas.js", Url)
}
<div class="box">
    <h2>Inicialización de Tablas</h2>
    @using (Html.BeginForm("Insertar", "Inicializar Tablas", FormMethod.Post, new { Id = "FormIndexInicializarTablas" }))
    {
        <table align="center" style="width: 930px;">
            <tr>
                <td>
                    <fieldset style="font-family: Arial, Helvetica, sans-serif; font-size: 12px; width: 925px;
                        text-align: center;">
                        <legend style="color: #FF9900"><b>Funcionalidad para Inicializar Tablas de la Base de Datos</b></legend>
                        <div class="div-filtro">
                            <div class="div-filtro-btn">
                                <p>
                                    <a class="btnIngresar" href="#" id="Insertar">Inicializar Tablas</a>
                                </p>
                            </div>
                        </div>
                    </fieldset>
                </td>
            </tr>
        </table>
        <table>
            <tr>
                <td style="font-weight: bold">
                    <div id="titulo">
                    </div>
                    <div id="DivSuperviciones">
                        @Html.Partial("_Lista", Model)
                    </div>
                </td>
            </tr>
        </table>
    }
</div>
