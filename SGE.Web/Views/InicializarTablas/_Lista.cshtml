@model InicializarTablasVista
@if (Model.Inserciones != null && Model.Inserciones.Count > 0)
{ 
    <div style="text-align:center;">
        <table id="grilla" align="center" width="925px" cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th height="30" width="45%">
                        <div align="center">Inserciones</div>
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Inserciones)
                {
                    <tr>
                        <td align="left" width="30%" style="padding-left:3%">
                            @item.ToMaxLenght(100)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
